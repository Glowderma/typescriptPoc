{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
  
    "rules": {
      "no-unused-expressions": "error",
      "@typescript-eslint/no-require-imports": "error",
      "no-undef": "off",
      "no-console": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
  }
  